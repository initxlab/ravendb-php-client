{
    "name": "initxlab/ravendb",
    "description": "RavenDB PHP API Client",
    "type": "project",
    "require": {
      "php": "^8.0",
      "ext-curl": "*",
      "doctrine/annotations": "^1.11.1",
      "ext-json": "*",
       "phpdocumentor/reflection-docblock": "^5.2",
      "ext-http": "*"
    },
    "autoload": {
        "psr-4": {
            "RavenDB\\": "ravendb/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RavenDB\\Tests\\": "ravendb/tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "5.x-dev"
    },
    "license": "MIT",
    "minimum-stability": "dev"
}
