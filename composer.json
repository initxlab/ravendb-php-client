{
  "name": "initxlab/ravendb",
  "description": "RavenDB PHP API Client",
  "type": "project",
  "require": {
    "php": "^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-http": "*",
    "spatie/async": "^1.5.3",
    "symfony/http-client": "^5.2.3",
    "symfony/stopwatch": "5.x-dev"
  },
  "autoload": {
    "psr-4": {
      "RavenDB\\": "src/ravendb/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RavenDB\\Tests\\": "src/tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/var-dumper": "5.x-dev",
    "ext-zend-opcache": "*"
  },
  "license": "MIT",
  "minimum-stability": "dev"
}
