{
  "name": "initxlab/ravendb",
  "description": "RavenDB PHP API Client",
  "type": "project",
  "require": {
    "php": "^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "symfony/http-client": "^5.2",
    "analog/analog": "^1.0",
    "ramsey/uuid": "^4.1.1",
    "symfony/serializer": "^5.2",
    "symfony/property-access": "^5.2",
    "doctrine/annotations": "^1.12",
    "doctrine/lexer": "^1.2",
    "doctrine/orm": "^2.8.2",
    "symfony/property-info": "^5.2",
    "nahid/jsonq": "dev-master",
    "symfony/event-dispatcher": "5.x-dev",
    "symfony/validator": "5.x-dev",
    "symfony/stopwatch": "5.x-dev",
    "vimeo/psalm": "4.x-dev",
    "php-ds/php-ds": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "RavenDB\\": "src/ravendb/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RavenDB\\Tests\\": "src/tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "symfony/var-dumper": "^5.2"
  },
  "license": "MIT",
  "minimum-stability": "dev"
}
