{
    "name": "initxlab/ravendb",
    "description": "RavenDB PHP API Client",
    "type": "project",
    "require": {
      "php": "^7.4|^8.0",
      "ext-curl": "*",
      "doctrine/annotations": "^1.11.1",
      "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "RavenDB\\": "ravendb/src/",
            "RavenDB\\Tests\\": "ravendb/tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RavenDB\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "minimum-stability": "dev"
}
